@article{Whitall2016,
  title = {Contaminants in {{Queen Conch}} ({{Strombus}} Gigas) in {{Vieques}}, {{Puerto Rico}}},
  author = {Whitall, David and Ramos, Antares and Wehner, Diane and Fulton, Michael and Mason, Andrew and Wirth, Ed and West, Blaine and Pait, Anthony and Pisarski, Emily and Shaddrix, Brian and Reed, Lou Ann},
  year = {2016},
